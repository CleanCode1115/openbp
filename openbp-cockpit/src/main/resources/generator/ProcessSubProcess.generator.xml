<generator
	name="ProcessSubprocess"
	display-name="Sub process"
	item-generator="true"
	sequence-nr="2"
	xml-sequence="13"
	item-type="Process"
	generator-customizer="org.openbp.cockpit.generator.process.ProcessCustomizer"
	>

	<description>Sub processes are called from within other processes. They encapsulate detail functionality that does not need to be visible within the calling process.
A sub process contains a single entry and a single final node by default. All entry and final nodes may have parameters.</description>

	<initial-property name="ProcessType">
		<value>Subprocess</value>
	</initial-property>

</generator>
