<generator
	name="ProcessBusinessProcess"
	display-name="Business process"
	item-generator="true"
	sequence-nr="3"
	xml-sequence="10"
	item-type="Process"
	generator-customizer="org.openbp.cockpit.generator.process.ProcessCustomizer"
	>

	<description>Business processes describe application functionality on an rather abstract level. A business process may contain portions that are not executable by the process engine.
Such a process has a structure that is similar to a top level process except that its notation resembles rather a flowchart instead of an UML activity diagram.</description>

	<initial-property name="SkinName">
		<value>flowchart</value>
	</initial-property>
	<initial-property name="ProcessType">
		<value>BusinessProcess</value>
	</initial-property>

</generator>
