<generator
	name="ProcessTopLevel"
	display-name="Top level process"
	item-generator="true"
	sequence-nr="1"
	xml-sequence="14"
	item-type="Process"
	generator-customizer="org.openbp.cockpit.generator.process.ProcessCustomizer"
	>

	<description>Top level processes define processes or use cases within the application that can be executed by the process engine.
By default, a top level process contains a single initial node.
More detailed functionality may be contained in sub processes, which are called by top level processes.</description>

	<initial-property name="ProcessType">
		<value>TopLevel</value>
	</initial-property>

</generator>
