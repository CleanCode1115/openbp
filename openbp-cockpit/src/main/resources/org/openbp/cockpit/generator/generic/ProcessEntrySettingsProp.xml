<object-descriptor
	name="ProcessEntrySettings"
	display-name="Process entries"
	object-class="org.openbp.cockpit.generator.generic.ProcessEntrySettings"
	>
	<description>Here you can edit the process entries.
In order to add or remove entres to the list, right-click an entry in the entry list and choose the option from the context menu.</description>

	<property name="EntryList" display-name="Process entries"
		expanded="true">
		<description>List of process entries</description>
		<collection
			type-class="org.openbp.cockpit.generator.generic.ProcessEntry"
			add-enabled="true"
			delete-enabled="true"
			reorder-enabled="true"/>
	</property>

</object-descriptor>
