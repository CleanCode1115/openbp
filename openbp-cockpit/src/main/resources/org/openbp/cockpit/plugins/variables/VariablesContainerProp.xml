<object-descriptor
	name="VariablesContainer"
	display-name="Process variables"
	object-class="org.openbp.cockpit.plugins.variables.VariablesContainer"
	validator="org.openbp.cockpit.plugins.variables.VariablesContainerValidator"
	>
	<description>Process variables can hold data values during the execution of a process.</description>

	<property name="ProcessVariableList" display-name="Process variables">
		<description>Process variables</description>
		<collection
			type-class="org.openbp.core.model.item.process.ProcessVariable"
			add-enabled="true"
			delete-enabled="true"
			reorder-enabled="true"/>
	</property>

</object-descriptor>
