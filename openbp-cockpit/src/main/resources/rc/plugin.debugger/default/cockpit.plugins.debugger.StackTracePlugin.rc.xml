<resource>
	<item name="title">Stack Trace</item>
	<item name="description">Displays the process stack trace of the token context</item>
	<item name="icon" type="image/multi">
		<path>../../images/plugins/inspector.png</path>
	</item>
	<item name="sequence">C-w k,C-w C-k</item>
	<item name="condition">features.debugger</item>

	<!-- Messages -->
	<item name="titlestatus">(listening as {0})</item>

	<item name="header.column1">Type</item>
	<item name="header.column2">Position</item>

	<item name="cause.current">Current position</item>
	<item name="cause.subprocess">Sub process</item>
</resource>
