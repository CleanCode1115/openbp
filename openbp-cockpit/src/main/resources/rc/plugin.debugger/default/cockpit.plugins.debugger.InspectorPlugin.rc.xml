<resource>
	<item name="title">Context Inspector</item>
	<item name="description">Displays live content of the token context of a process</item>
	<item name="icon" type="image/multi">
		<path>../../images/plugins/inspector.png</path>
	</item>
	<item name="sequence">A-F3,C-w d,C-w C-d</item>
	<item name="condition">features.debugger</item>

	<!-- Messages -->
	<item name="titlestatus">(listening as {0})</item>
	<group name="header">
		<item name="column1">Key</item>
		<item name="column2">Type</item>
		<item name="column3">Value</item>
	</group>
</resource>
