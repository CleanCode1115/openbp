<resource>
	<item name="title">Toolbox</item>
	<item name="description">User-configurable toolbox containing arbitrary components</item>
	<item name="icon" type="image/multi">
		<path>../../images/plugins/toolbox.png</path>
	</item>

	<!-- Actions -->
	<group name="toolbox.changename">
		<item name="displayname">Change name</item>
		<item name="description">Change the name of the toolbox</item>
	</group>

	<!-- Messages -->
	<item name="toolbox.dialog.changename.text">Enter the name of the toolbox</item>
	<item name="toolbox.empty">(empty)</item>

	<!-- Strings for color toolbox items -->
	<item name="color.title">Color</item>
	<item name="color.red">Red:</item>
	<item name="color.green">Green:</item>
	<item name="color.blue">Blue:</item>
</resource>
