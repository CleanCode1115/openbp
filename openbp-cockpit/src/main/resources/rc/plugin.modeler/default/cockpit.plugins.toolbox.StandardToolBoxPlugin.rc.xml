<resource>
	<import resource="cockpit.plugins.toolbox.ToolBoxPlugin" />

	<item name="title">Process Elements</item>
	<item name="description">Standard process element toolbox</item>
	<item name="icon" type="image/multi">
		<path>../../images/plugins/standardtoolbox.png</path>
	</item>
	<item name="sequence">S-F3,C-w s,C-w C-s</item>

	<group name="toolboxitem">

		<item name="initial.tooltip">Initial node</item>
		<item name="final.tooltip">Final node</item>
		<item name="decision.tooltip">Decision node</item>
		<item name="merge.tooltip">Merge node</item>
		<item name="fork.tooltip">Fork node</item>
		<item name="join.tooltip">Join node</item>
		<item name="waitstate.tooltip">Wait state node</item>
		<item name="workflow.tooltip">Workflow node</item>
		<item name="workflowend.tooltip">Workflow end node</item>
		<item name="textelement.tooltip">Text element</item>
		<item name="hline.tooltip">Horizontal swimlane line</item>
		<item name="vline.tooltip">Vertical swimlane line</item>
		<item name="placeholder.tooltip">Placeholder</item>
		<item name="activity.tooltip">New activity</item>
		<item name="visual.tooltip">New visual</item>
		<item name="actor.tooltip">New actor</item>
		<item name="process.tooltip">New sub process</item>
		<item name="socketentry.tooltip">Entry socket</item>
		<item name="socketexit.tooltip">Exit socket</item>

	</group>
</resource>
