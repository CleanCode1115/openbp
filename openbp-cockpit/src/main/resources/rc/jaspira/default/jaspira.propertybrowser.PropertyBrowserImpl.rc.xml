<resource>

	<!-- Actions -->
	<group name="propertybrowser.addelement">
		<item name="displayname">_Add Element</item>
		<item name="description">Adds a new element to the end of the list</item>
		<item name="icon" type="image/multi">
			<path>../images/oeadd.png</path>
		</item>
		<item name="sequence">C-n</item>
	</group>

	<group name="propertybrowser.copyelement">
		<item name="displayname">_Copy Element</item>
		<item name="description">Copies the selected element</item>
		<item name="icon" type="image/multi">
			<path>../images/oecopy.png</path>
		</item>
		<item name="sequence">C-c</item>
	</group>

	<group name="propertybrowser.cutelement">
		<item name="displayname">C_ut Element</item>
		<item name="description">Cuts the selected element</item>
		<item name="icon" type="image/multi">
			<path>../images/oecut.png</path>
		</item>
		<item name="sequence">C-x</item>
	</group>

	<group name="propertybrowser.pasteelement">
		<item name="displayname">_Paste Element</item>
		<item name="description">Pastes a copied element</item>
		<item name="icon" type="image/multi">
			<path>../images/oepaste.png</path>
		</item>
		<item name="sequence">C-v</item>
	</group>

	<group name="propertybrowser.removeelement">
		<item name="displayname">_Remove Element</item>
		<item name="description">Removes the selected element</item>
		<item name="icon" type="image/multi">
			<path>../images/oeremove.png</path>
		</item>
		<item name="sequence">DELETE</item>
	</group>

	<group name="propertybrowser.moveelementup">
		<item name="displayname">Move Element _Up</item>
		<item name="description">Moves the element upward in the list</item>
		<item name="icon" type="image/multi">
			<path>../images/oeup.png</path>
		</item>
		<item name="sequence">C-u</item>
	</group>

	<group name="propertybrowser.moveelementdown">
		<item name="displayname">Move Element _Down</item>
		<item name="description">Moves the element downward in the list</item>
		<item name="icon" type="image/multi">
			<path>../images/oedown.png</path>
		</item>
		<item name="sequence">C-d</item>
	</group>

	<!-- Table headers -->
	<group name="propertybrowser.header">
		<item name="columnproperty">Property</item>
		<item name="columndescription">Description</item>
		<item name="columnvalue">Value</item>
	</group>

	<!-- Messages -->
	<item name="propertybrowser.removeall">Do you really want to remove all elements of the collection?</item>

</resource>
