<object-descriptor
	name="ModelObject"
	display-name="Node socket"
	object-class="org.openbp.core.model.ModelObject"
	validator="org.openbp.guiclient.objectvalidators.ModelObjectValidator"
	>
	<description></description>

	<property name="Name"/>
	<property name="DisplayName"/>
	<property name="Description"/>

	<property name="TaggedValueList" display-name="Tagged values"
		expanded="true">
		<description>List of arbitrary tagged values</description>
		<collection
			type-class="org.openbp.common.generic.taggedvalue.TaggedValue"
			add-enabled="true"
			delete-enabled="true"
			reorder-enabled="true"/>
	</property>

</object-descriptor>
