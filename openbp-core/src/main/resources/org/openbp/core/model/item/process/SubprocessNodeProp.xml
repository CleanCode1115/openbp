<object-descriptor
	name="SubprocessNode"
	display-name="Sub process node"
	object-class="org.openbp.core.model.item.process.SubprocessNode"
	>
	<description>Calls a sub process</description>

	<property name="Name"/>
	<property name="DisplayName"/>
	<property name="Description"/>

	<property name="SubprocessName" display-name="Sub process" read-only="true"
		editor="StringEditor">
		<description>Name of the process to call</description>
	</property>

	<property name="EventHandlerDefinition"/>

	<property name="ImagePath"/>
	<property name="ImageOnly"/>
	<property name="ImageResize"/>

	<property name="TaggedValueList"/>

</object-descriptor>
