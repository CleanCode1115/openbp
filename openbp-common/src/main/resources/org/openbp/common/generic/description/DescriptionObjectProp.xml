<object-descriptor
	name="DescriptionObject"
	display-name="Description Object"
	object-class="org.openbp.common.generic.description.DescriptionObject"
	>
	<description>Generic object that has a name and a description.</description>

	<property name="Name" display-name="System name"
		editor="StringEditor"
		required="true">
		<description>Name of the object</description>
	</property>

	<property name="Description" display-name="Description"
		editor="MultiLineStringEditor">
		<description>Description</description>
	</property>

</object-descriptor>
