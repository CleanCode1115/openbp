<resource>

	<item name="defaulttitle">OpenBP</item>

	<!-- The buttons -->	
	<group name="buttons">
		<group name="ok">
			<item name="caption">_Ok</item>
		</group>

		<group name="cancel">
			<item name="caption">_Cancel</item>
		</group>

		<group name="yes">
			<item name="caption">_Yes</item>
		</group>

		<group name="no">
			<item name="caption">_No</item>
		</group>

		<group name="yesforall">
			<item name="caption">Yes for _all</item>
		</group>

		<group name="noforall">
			<item name="caption">No _for all</item>
		</group>

		<group name="abort">
			<item name="caption">_Abort</item>
		</group>

		<group name="retry">
			<item name="caption">_Retry</item>
		</group>

		<group name="ignore">
			<item name="caption">_Ignore</item>
		</group>

		<group name="apply">
			<item name="caption">_Apply</item>
		</group>
