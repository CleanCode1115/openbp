			<path>mytesticons/Info.gif</path>
		</item>

		<item name="warning" type="image/multi">
			<path>mytesticons/Warn.gif</path>
		</item>

		<item name="error" type="image/multi">
			<path>mytesticons/Error.gif</path>	
		</item>

		<item name="fatal" type="image/multi">
